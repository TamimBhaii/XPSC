#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios:: sync_with_stdio(false);
    cin.tie(nullptr);
    
    long t;
    cin >> t;
    while(t--)
    {
        long n;
        cin >> n;
        n = (n > 0 ? n : -n);
        cout << (n % 2 ? "Kosuke" : "Sakurako") << endl;
    }

    return 0;
}
