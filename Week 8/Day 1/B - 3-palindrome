#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios:: sync_with_stdio(false);
    cin.tie(nullptr);
    
    long long n, j=0;
    cin >> n;
    string s[] = {"a", "a", "b", "b"};

    for(int i = 0; i < n; i++)
    {
        cout << s[j];
        j++;
        if(j == 4)
        {
            j = 0;
        }
    }
    cout << endl;
    
    return 0;
}
