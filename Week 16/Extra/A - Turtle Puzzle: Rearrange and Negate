#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    
    long t;
    cin >> t;
    while(t--)
    {
        long n, cs = 0;
        cin >> n;
        while(n--)
        {
            long x;
            cin >> x;
            cs += (x > 0 ? x : -x);
        }
        cout << cs << endl;
    }
    
    return 0;
}
