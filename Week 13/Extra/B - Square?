#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin >> t;
    while(t--){
        long long a, a1, b, b1;
        cin >> a >> b >> a1 >> b1;
        if(a > b){
            swap(a, b);
        }
        if(a1 > b1){
            swap(a1, b1);
        }
        if(a + a1 == b && a + a1 == b1){
            cout << "YES" << endl;
        }
        else{
            cout << "NO" << endl;
        }
    }
    return 0;
}
