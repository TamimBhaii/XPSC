#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios:: sync_with_stdio(false);
    cin.tie(nullptr);
    
    long long t;
    cin >> t;
    while(t--) 
    {
        long long n, k;
        cin >> n >> k;
        for(int i = 0; i < n; i++) 
        {
            for(char ch = 'a'; ch < 'a' + k; ch++) 
            {
                cout << ch;
            }
        }
        cout << endl;
    }

    return 0;
}
