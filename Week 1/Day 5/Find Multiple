#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios:: sync_with_stdio(false);
    cin.tie(nullptr);
    
    int a,b,c;
    cin >> a >> b >> c;

    int mult = ((a+c-1)/c)*c;
    if(mult<=b)
    {
        cout << mult << endl;
    }
    else 
        cout << -1 << endl;
    
    return 0;
}
