#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    long long t;
    cin >> t;
    while (t--)
    {
        long long n, m, p;
        cin >> n >> m >> p;
        cout << max((n + m - 1) / m, (n + p - 1) / p) << endl;
    }
    return 0;
}
