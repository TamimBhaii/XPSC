#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios:: sync_with_stdio(false);
    cin.tie(nullptr);
    
    int n;
    cin >> n;
    vector<int> a(n);
    for(int i = 0; i<n; i++)
    {
        cin >> a[i];
    }
    sort(a.begin(), a.end());
    
    int m;
    cin >> m;
    vector<int>v(m);
    for(int i = 0; i<m; i++)
    {
        int l,r;
        cin >> l >> r;

        int low = lower_bound(a.begin(), a.end(),l) - a.begin();
        int upp = upper_bound(a.begin(), a.end(), r) - a.begin();
        v[i] = upp - low;
    }

    for(int i = 0; i<m; i++)
    {
        cout << v[i] << " ";
    }
    cout << endl;
    
    return 0;
}
