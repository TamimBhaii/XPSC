#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios:: sync_with_stdio(false);
    cin.tie(nullptr);
    
    int t;
    cin >> t;
    while(t--)
    {
        int n, q;
        cin >> n >> q;
        vector<int> a(n);
        for(int i = 0; i < n; i++)
        {
            cin >> a[i];
        }

        while(q--)
        {
            int l, r;
            cin >> l >> r;
            l--, r--;
            vector<int> v(a.begin() + l, a.begin() + r + 1);
            sort(v.rbegin(), v.rend());

            long long x = 0, y = 0;
            for(int i = 0; i < v.size(); i++)
            {
                if(i % 2 == 0)
                {
                    x += v[i];
                }
                else
                {
                    y += v[i];
                }
            }

            if(y >= x)
            {
                cout << "YES" << endl;
            }
            else
            {
                cout << "NO" << endl;
            }
        }
    }

    return 0;
}
