#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios:: sync_with_stdio(false);
    cin.tie(nullptr);

    string S, T;
    cin >> S >> T;
    
    int cnt = 0;
    for (int i = 0; i < S.length(); i++) {
        if (S[i] != T[i]) {
            cnt++;
        }
    }

    cout << cnt << endl;
    
    return 0;
}
