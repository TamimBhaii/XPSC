#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    string cor = "Timur";
    sort(cor.begin(), cor.end());

    while (t--)
    {
        int n;
        string s;
        cin >> n >> s;

        if (n != 5)
        {
            cout << "NO" << endl;
        }
        else
        {
            string str = s;
            sort(str.begin(), str.end());

            if (str == cor)
            {
                cout << "YES" << endl;
            }
            else
            {
                cout << "NO" << endl;
            }
        }
    }

    return 0;
}
