#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios:: sync_with_stdio(false);
    cin.tie(nullptr);
    
    int t;
    cin >> t;
    while(t--)
    {
        int n, m;
        cin >> n >> m;

        for(int i = n-m; i<=n; i++)
        {
            cout << i << " ";
        }
        for(int i = n-m-1; i>=1; i--)
        {
            cout << i << " ";
        }
        cout << endl;
    }
    
    return 0;
}
